generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Template {
  id        String   @id @default(uuid())
  name      String   @unique
  type      String?
  isDeleted Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("Template")
}
